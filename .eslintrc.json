{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "linebreak-style": ["error", "windows"]
  }
}
